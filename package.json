{
  "name": "synthesis-engine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:benchmark-governance": "TMPDIR=/tmp vitest run src/lib/services/__tests__/benchmark-governance.test.ts",
    "test:scm-promotion-governance": "TMPDIR=/tmp vitest run src/lib/services/__tests__/scm-promotion-governance.test.ts",
    "test:scientific-integrity": "TMPDIR=/tmp vitest run src/lib/services/__tests__/scientific-integrity-service.test.ts",
    "seed:framework-scms": "node scripts/seed-framework-scms.mjs",
    "promote:scm": "npx --yes tsx scripts/promote-scm-model.ts",
    "validate:schemas": "node scripts/validate-causal-graph-schema.mjs --all",
    "validate:consistency": "node scripts/validate-scm-consistency.mjs --all",
    "validate:all": "npm run validate:schemas && npm run validate:consistency",
    "governance:claim-drift": "npx --yes tsx scripts/scan-claim-drift.ts",
    "governance:claim-reconcile": "npx --yes tsx scripts/reconcile-claim-ledger.ts",
    "governance:claim-migrate": "npx --yes tsx scripts/migrate-claim-ledger.ts",
    "governance:policy-eval": "npx --yes tsx scripts/evaluate-experiment-policies.ts",
    "governance:causal-method": "npx --yes tsx scripts/evaluate-causal-method-selection.ts",
    "governance:causal-method-eval": "npm run governance:causal-method --",
    "governance:uncertainty-cal": "npx --yes tsx scripts/evaluate-uncertainty-calibration.ts",
    "governance:uncertainty-calibration": "npm run governance:uncertainty-cal --",
    "governance:law-falsification": "npx --yes tsx scripts/evaluate-law-falsification.ts",
    "governance:hybrid-novelty-proof": "npx --yes tsx scripts/evaluate-hybrid-novelty-proof.ts",
    "governance:persistent-memory-integrity": "npx --yes tsx scripts/evaluate-persistent-memory-integrity.ts",
    "audit:migrations": "npx --yes tsx scripts/check-migration-state.ts",
    "agent:bootstrap": "bash ../.agent/scripts/agent-bootstrap.sh",
    "test": "TMPDIR=/tmp npx vitest run"
  },
  "dependencies": {
